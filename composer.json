{
  "name": "mantax559/laravel-helpers",
  "description": "Laravel Helpers",
  "type": "library",
  "license": "MIT",
  "keywords": [],
  "authors": [
    {
      "name": "mantax559"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.8",
    "laravel/framework": "^9.0|^10.0",
    "spatie/laravel-collection-macros": "^7.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mantax559\\LaravelHelpers\\Providers\\AppServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
