{
  "name": "mantax559/laravel-helpers",
  "description": "Laravel Helpers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "mantax559"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^8.0|^9.0|^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
