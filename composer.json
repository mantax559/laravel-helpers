{
  "name": "mantax559/laravel-helpers",
  "description": "Laravel Helpers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "mantax559"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "laravel/framework": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mantax559\\LaravelHelpers\\PackageServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
