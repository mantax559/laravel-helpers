{
  "name": "mantax559/laravel-helpers",
  "description": "Laravel Helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "mantax559"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Mantax559\\LaravelHelpers\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mantax559\\LaravelHelpers\\PackageServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
